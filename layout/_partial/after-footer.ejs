<!-- Disqus Comments -->
<% if (theme.comments && theme.comments.disqus_shortname){ %>
<script type="text/javascript">
    var disqus_shortname = '<%= theme.comments.disqus_shortname %>';
    var disqus_config = function() {
        this.page.url = '<%= page.permalink %>';
        this.page.identifier = document.location.pathname;
    };
    (function(){
        var dsq = document.createElement('script');
        dsq.type = 'text/javascript';
        dsq.async = true;
        dsq.src = '//' + disqus_shortname + '.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    }());
</script>
<% } %>

<%- js('js/typed.min.js') %>


<script>
    $(function() {
        $('#things-i-do').typed({
            strings  : ['cycling Tel Aviv.', 'surfing at sunset.', 'playing my guitar.', 'boozing away.', 'chasing my kids.', 'teasing my wife.'],
            typeSpeed: 20,
            backSpeed: 20,
            startDelay: 100,
            backDelay: 1000,
            loop     : true
        });
    })
</script>